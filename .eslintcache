;\n  h2 {\n    position: absolute;\n    top: 10%;\n    left: 50%;\n    transform: translate(-50%, -10%);\n    @media (max-width: 1500px) {\n      font-size: 3rem;\n    }\n  }\n\n  img {\n    width: 100%;\n    height: 70vh;\n    object-fit: cover;\n  }\n`;\nconst Awards = styled.div`\n  min-height: 80vh;\n  display: flex;\n  margin: 5rem 10rem;\n  align-items: center;\n  justify-content: space-around;\n  @media (max-width: 1300px) {\n    display: block;\n    margin: 2rem 2rem;\n  }\n`;\nconst AwardStyle = styled.div`\n  padding: 5rem;\n  h3 {\n    font-size: 2rem;\n  }\n  .line {\n    width: 100%;\n    background: #23d997;\n    margin: 1rem 0rem;\n    height: 0.5rem;\n  }\n`;\nconst ImageDisplay = styled.div`\n  min-height: 50vh;\n  img {\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n`;\n// Award Component\nconst Award = ({ title, description }) => {\n  return (\n    <AwardStyle>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description} </p>\n    </AwardStyle>\n  );\n};\nexport default MovieDetail;\n","/Users/faisalal-qabbani/Desktop/web/reactjs/my-site/capture/src/movieState.js",[],"/Users/faisalal-qabbani/Desktop/web/reactjs/my-site/capture/src/animation.js",[],"/Users/faisalal-qabbani/Desktop/web/reactjs/my-site/capture/src/components/Wave.js",[],"/Users/faisalal-qabbani/Desktop/web/reactjs/my-site/capture/src/components/Toggle.js",[],"/Users/faisalal-qabbani/Desktop/web/reactjs/my-site/capture/src/components/useScroll.js",[],"/Users/faisalal-qabbani/Desktop/web/reactjs/my-site/capture/src/components/ScrollTop.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":10,"column":18,"nodeType":"85","messageId":"86","endLine":10,"endColumn":27},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'setMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]